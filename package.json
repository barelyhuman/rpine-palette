{
	"name": "@rose-pine/palette",
	"version": "1.0.0",
	"description": "Color palette tool for Rosé Pine",
	"license": "MIT",
	"repository": "rose-pine/palette",
	"funding": "https://github.com/rose-pine/palette?sponsor=1",
	"author": {
		"name": "Rosé Pine",
		"email": "hi@rosepinetheme.com"
	},
	"type": "module",
	"exports": "./dist/index.js",
	"engines": {
		"node": ">=12"
	},
	"scripts": {
		"build": "del-cli dist && tsc && npm test",
		"test": "xo && ava",
		"release": "np",
		"prepare": "npm run build"
	},
	"files": [
		"dist"
	],
	"types": "dist",
	"keywords": [
		"theme",
		"palette",
		"soho-vibes",
		"rose-pine"
	],
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@mvllow/tsconfig": "^0.1.0",
		"@types/node": "^16.11.11",
		"ava": "^3.15.0",
		"del-cli": "^4.0.1",
		"np": "^7.6.0",
		"prettier": "^2.5.0",
		"ts-node": "^10.4.0",
		"typescript": "^4.5.2",
		"xo": "^0.47.0"
	},
	"prettier": {
		"bracketSpacing": false,
		"semi": false,
		"singleQuote": true,
		"trailingComma": "es5",
		"useTabs": true
	},
	"xo": {
		"prettier": true
	},
	"ava": {
		"extensions": {
			"ts": "module"
		},
		"nonSemVerExperiments": {
			"configurableModuleFormat": true
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	}
}
